{
  "expo": {
    "name": "Maya",
    "slug": "maya-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "maya",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo.jpeg ",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maya.app",
      "buildNumber": "1.0.0",
      "deploymentTarget": "17.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Maya a besoin d'accéder à votre caméra pour scanner les codes QR.",
        "NSLocationWhenInUseUsageDescription": "Maya a besoin de votre localisation pour vous montrer les magasins à proximité.",
        "NSPhotoLibraryUsageDescription": "Maya a besoin d'accéder à vos photos pour partager des contenus.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.maya.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "googleClientId": "535870809549-kanp7rd1hmu5ubq88aejlg2pk78htjhi.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "70138919-2611-4fa0-b38a-90919894c002"
      }
    }
  }
}
